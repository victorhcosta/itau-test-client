{
	"extends": [
		"stylelint-config-recommended"
	  ],
	  "plugins": [
		  "stylelint-scss",
		  "stylelint-order"
	  ],
	  "rules": {
		  "indentation": "tab",
		  "at-rule-no-unknown": null,
		  "scss/at-rule-no-unknown": true,
		  "scss/selector-no-redundant-nesting-selector": true,
		  "no-empty-source": null
	  }
}
